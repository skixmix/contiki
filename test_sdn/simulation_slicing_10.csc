    <location_x>400</location_x>
    <location_y>2</location_y>
    <minimized>true</minimized>
  </plugin>
  <plugin>
    org.contikios.cooja.plugins.ScriptRunner
    <plugin_config>
      <script>TIMEOUT(3600000, for (var ids in outputs){outputs[ids].close();}); //1 hour timeout

sim.setSpeedLimit(5.0); //500%

//import Java Package to JavaScript
 importPackage(java.io);
 
 // Use JavaScript object as an associative array
 outputs = new Object();
 
 var path = "/home/user/Desktop/risultati_esperimento/";
 
 while (true) {
    YIELD(); /* wait for another mote output */
    
 	//Has the output file been created.
 	if(! outputs[id.toString()]){
 		// Open log_&lt;id&gt;.txt for writing.
 		// BTW: FileWriter seems to be buffered.
 		outputs[id.toString()]= new FileWriter(path + "node" + id +".log");
 	}
 	//Write to file.
 	outputs[id.toString()].write(time + ":" + id + ":" + msg + "\n");
 }</script>
      <active>true</active>
    </plugin_config>
