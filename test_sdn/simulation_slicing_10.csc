  <plugin>
    org.contikios.cooja.plugins.ScriptRunner
    <plugin_config>
      <script>//import Java Package to JavaScript
 importPackage(java.io);
 
//Simulation setup

//Duration in milliseconds (1 hour = 3600 000ms)
TIMEOUT(3600000, for (var ids in outputs){outputs[ids].close();}); 

sim.setSpeedLimit(5.0); //400% speed means 1 simultaion hour will be 15 minutes in real time

var exp_path = "/home/user/Desktop/risultati_esperimento/";

//Logging
 // Use JavaScript object as an associative array
 outputs = new Object();
 
 while (true) {
 	//Has the output file been created.
 	if(!outputs[id.toString()]){
 		outputs[id.toString()]= new FileWriter(exp_path + "node" + id +".log");
 	}
 	//Write to file.
 	outputs[id.toString()].write(time + ":" + id + ":" + msg + "\n");
 	YIELD();
 }</script>
      <active>false</active>
    </plugin_config>
    <width>1027</width>
    <z>-1</z>
